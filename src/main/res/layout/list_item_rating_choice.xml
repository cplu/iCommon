<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				android:layout_width="match_parent"
				android:layout_height="@dimen/ui_preferred_subtitle_height">
	<CheckBox
		android:id="@+id/checkbox_rating_choice"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentLeft="true"
		android:layout_centerVertical="true"
		android:layout_marginLeft="@dimen/large_margin"
		android:paddingLeft="@dimen/padding_checkbox"
		android:button="@drawable/selector_icon_rating_choice"
		android:textColor="@color/grayscale1"
		android:textSize="@dimen/text_size_f2"
		android:singleLine="true"
		/>
</RelativeLayout>