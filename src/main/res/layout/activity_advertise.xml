<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				android:id="@+id/activity_ad"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@android:color/transparent">

	<ImageView
		android:id="@+id/image_view_ad"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:scaleType="fitCenter"
		/>

	<TextView
		android:id="@+id/text_view_close"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentTop="true"
		android:layout_alignParentRight="true"
		android:layout_marginRight="@dimen/default_margin_2x"
		android:layout_marginTop="@dimen/default_margin_2x"
		android:padding="@dimen/small_padding"
		android:gravity="center"
		android:text="@string/click_to_skip"
		android:textColor="@color/default_white"
		android:textSize="@dimen/text_size_f2"
		android:includeFontPadding="false"
		android:background="@drawable/bg_rounded_transparent_78"
		/>

	<!--<ProgressBar-->
		<!--android:id="@+id/progress_ad"-->
		<!--style="?android:attr/progressBarStyle"-->
		<!--android:layout_width="wrap_content"-->
		<!--android:layout_height="wrap_content"-->
		<!--android:layout_centerInParent="true"/>-->
</RelativeLayout>
